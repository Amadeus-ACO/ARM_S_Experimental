   <left>
      <Region id="left" onDragDropped="#onRegionDragDropped" onDragEntered="#onRegionDragEntered" onDragExited="#onRegionDragExited" onDragOver="#onRegionDragOver" prefWidth="0" style="-fx-background-color: ghostwhite;" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Region id="right" onDragDropped="#onRegionDragDropped" onDragEntered="#onRegionDragEntered" onDragExited="#onRegionDragExited" onDragOver="#onRegionDragOver" prefWidth="0" style="-fx-background-color: ghostwhite;" BorderPane.alignment="CENTER" />
   </right>
   <bottom>
      <Region id="bottom" onDragDropped="#onRegionDragDropped" onDragEntered="#onRegionDragEntered" onDragExited="#onRegionDragExited" onDragOver="#onRegionDragOver" prefHeight="0" style="-fx-background-color: ghostwhite;" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
